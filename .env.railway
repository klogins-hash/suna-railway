# Railway Environment Variables Template
# Copy these to your Railway project environment variables

# Environment Mode
ENV_MODE=production

# Database - Supabase (original setup)
SUPABASE_URL=your_supabase_url_here
SUPABASE_ANON_KEY=your_supabase_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key_here

# Redis (Railway will provide these automatically if you add Redis service)
REDIS_HOST=${{Redis.RAILWAY_PRIVATE_DOMAIN}}
REDIS_PORT=${{Redis.RAILWAY_TCP_PROXY_PORT}}
REDIS_PASSWORD=
REDIS_SSL=false

# LLM Providers (Required - add your API keys)
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
MODEL_TO_USE=claude-3-5-sonnet-20241022

# AWS (Optional)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION_NAME=us-east-1

# Additional LLM Providers (Optional)
GROQ_API_KEY=your_groq_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here
MORPH_API_KEY=your_morph_api_key_here

# OpenAI Compatible APIs (Optional)
OPENAI_COMPATIBLE_API_KEY=your_compatible_api_key_here
OPENAI_COMPATIBLE_API_BASE=your_compatible_api_base_here

# Data APIs (Optional)
RAPID_API_KEY=your_rapid_api_key_here

# Web Search (Recommended)
TAVILY_API_KEY=your_tavily_api_key_here

# Web Scraping (Recommended)
FIRECRAWL_API_KEY=your_firecrawl_api_key_here
FIRECRAWL_URL=https://api.firecrawl.dev

# Sandbox Container Provider (Required for code execution)
DAYTONA_API_KEY=your_daytona_api_key_here
DAYTONA_SERVER_URL=your_daytona_server_url_here
DAYTONA_TARGET=your_daytona_target_here

# Observability (Optional)
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Security
API_KEY_SECRET=your_random_secret_key_here
MCP_CREDENTIAL_ENCRYPTION_KEY=your_encryption_key_here

# Additional APIs (Optional)
SMITHERY_API_KEY=your_smithery_api_key_here

# Webhooks
WEBHOOK_BASE_URL=https://your-app-name.railway.app

# Slack Integration (Optional)
SLACK_CLIENT_ID=your_slack_client_id_here
SLACK_CLIENT_SECRET=your_slack_client_secret_here
SLACK_REDIRECT_URI=https://your-app-name.railway.app/auth/slack/callback

# Pipedream Integration (Optional)
PIPEDREAM_CLIENT_ID=your_pipedream_client_id_here
PIPEDREAM_CLIENT_SECRET=your_pipedream_client_secret_here
PIPEDREAM_ENVIRONMENT=production
PIPEDREAM_PROJECT_ID=your_pipedream_project_id_here

# Railway specific
PORT=8000
RAILWAY_ENVIRONMENT=production
